{
  "watch": [
    "server.js",
    "routes/",
    "controllers/",
    "middleware/",
    "models/",
    "config/",
    "utils/"
  ],
  "ignore": [
    "node_modules/",
    "public/",
    "*.log",
    ".git/",
    "*.test.js",
    "*.spec.js"
  ],
  "ext": "js,json",
  "delay": "2000",
  "env": {
    "NODE_ENV": "development"
  },
  "restartable": "rs",
  "verbose": true
}
